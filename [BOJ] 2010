import java.util.Scanner;

public class Main{
  public static void main(String[] args){

    int n,con,result=0; //멀티탭,콘센트 개수
    Scanner sc=new Scanner(System.in);

    n=sc.nextInt();

    for(int i=0;i<n;i++){
      con=sc.nextInt(); 
      result+=con; //콘센트 개수 더하기
    }
    System.out.println(result-n+1); //전체 플러그 개수-멀티탭 개수+플러그가 안 꽂히는 마지막 멀티탭
  }
}
