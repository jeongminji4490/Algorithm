import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    int num,a;
    Scanner sc=new Scanner(System.in);

    num=sc.nextInt();

    while(true)
    {
      a=sc.nextInt();

      if(a==0)
      break;

      if(a%num==0)
      System.out.println(a+" is a multiple of "+num+'.');
      else
      System.out.println(a+" is NOT a multiple of "+num+'.');
    }
  }
}
