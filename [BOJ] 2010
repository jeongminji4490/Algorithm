import java.util.Scanner;

public class Main{
  public static void main(String[] args){

    int n,con,result=0; //멀티탭,콘센트 개수
    Scanner sc=new Scanner(System.in);

    n=sc.nextInt();

    for(int i=0;i<n;i++){
      con=sc.nextInt(); 
      result+=con; //콘센트 개수 더하기
    }
    result-=(n-1);
    System.out.println(result);
  }
}
